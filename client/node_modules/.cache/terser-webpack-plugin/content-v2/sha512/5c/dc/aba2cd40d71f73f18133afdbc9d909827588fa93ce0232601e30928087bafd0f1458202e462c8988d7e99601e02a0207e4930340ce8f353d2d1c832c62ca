/adminLogin\", body);\r\n        //console.log(\"auth/actions: \", res.data);\r\n        dispatch({\r\n            type: LOGIN_SUCCESS,\r\n            payload: res.data\r\n        });\r\n        \r\n        dispatch(loadAdmin());\r\n        \r\n    } catch(err) {\r\n        const errors = err.response.data.errors;\r\n        let errMsg = '';\r\n        errors?.forEach(error => errMsg = (error.msg));\r\n        dispatch({\r\n            type: LOGIN_FAIL,\r\n            errMsg: errMsg\r\n        });\r\n    }\r\n\r\n\r\n}\r\n\r\n\r\n//logout\r\nexport const logout = () => dispatch => {\r\n    dispatch({ type: LOGOUT });\r\n}\r\n\r\n\r\n","import React from \"react\";\r\nimport \"./Navbar.css\";\r\nimport { Link, useHistory } from 'react-router-dom';\r\nimport { connect } from 'react-redux';\r\nimport PropTypes from 'prop-types';\r\nimport { logout } from \"../actions/auth\";\r\n\r\nconst Navbar = ({auth: {isAuthenticated, loading}, logout}) => {\r\n\r\n    const history = useHistory();\r\n\r\n\r\n    function handleLogout(e) {\r\n        e.preventDefault();\r\n        logout();\r\n        if(!loading && !isAuthenticated) {\r\n            history.push('/login');\r\n        }\r\n    }\r\n\r\n\r\n    return (\r\n        <div className = \"navbar\">\r\n\r\n            <div className=\"navbar__title\"> \r\n                <Link to='/' className=\"nav__title\" >\r\n                    Rocks & Logs\r\n                </Link>               \r\n                               \r\n            </div>\r\n\r\n\r\n\r\n            <div className=\"navbar__right\"> \r\n                <div className=\"nav__eles\" >\r\n                    <Link to='/add-site' className=\"nav__ele\"  ><h2>Add/Del Site</h2></Link>\r\n\r\n                    <Link to='/workers' className=\"nav__ele\"  ><h2>Workers</h2></Link>     \r\n\r\n                    <Link to='/monthly-report' className=\"nav__ele\"  ><h2>Monthly Report</h2></Link> \r\n                </div>\r\n            \r\n                      